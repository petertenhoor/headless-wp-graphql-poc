{
  "apps": [
    {
      "name": "wp-graphql-proxy",
      "script": "./index.js",
      "instances": "1",
      "exec_mode": "fork",
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "error_file": "./logs/apiErrors.log",
      "out_file": "./logs/apiOutput.log",
      "log_date_format": "DD-MM-YYYY HH:mm Z"
    }
  ]
}
